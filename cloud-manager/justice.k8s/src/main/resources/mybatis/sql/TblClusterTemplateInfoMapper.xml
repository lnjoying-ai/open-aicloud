  <select id="selectVersionsByClusterId" resultType="com.lnjoying.justice.cluster.manager.db.model.TblClusterTemplateInfo$ClusterTemplateVersion">
    SELECT
    t1.ID as templateId,
    t1.NAME AS templateName,
    t2."version" AS "version",
    t2.version_id as VersionId
    FROM
    (
    ( SELECT NAME, ID FROM tbl_cluster_template_info
    <where>
      <if test="clusterId != null and clusterId != ''">
        and cluster_id=#{clusterId,jdbcType=VARCHAR}
      </if>
    </where>
     ) t1
    LEFT JOIN ( SELECT version_id, template_id, "version", update_time, create_time FROM tbl_cluster_tmpl_ver_info ) t2 ON t1.ID = t2.template_id
    )
    ORDER BY
    t2.update_time,
    t2.create_time
